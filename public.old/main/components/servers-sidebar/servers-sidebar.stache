{{#appState}}

<h4>Servers <i class="mdi-content-add pull-right" data-toggle="modal" data-target="#addServerModal"></i></h4>
<ul class="list-unstyled">
  {{#servers}}
  <li can-click="selectServer">
    <p class="hostname">{{name}}</p>
    <p class="type">{{type}}</p>
  </li>
  {{/servers}}
</ul>

{{/appState}}

