<ul class="list-accordion tree-style">
  <li><a href="/" {{#routeCurrent(page='dashboard')}}class="active"{{/routeCurrent}}><i class="zmdi zmdi-view-carousel"></i> Dashboard</a></li>
  <li class="list-title">Servers</li>
  {{^if servers.isPending}}
    {{#each servers}}
      <li><a href="/server/{{name}}" {{#if currentServer(name)}}class="active"{{/if}}>
          <i class="zmdi zmdi-storage"></i>{{name}}</a>
      </li>
    {{/each}}
  {{/if}}
</ul>
